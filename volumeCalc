#include <stdio.h>

int main(){

    float contheight, radius, volume;

    printf("Insert, in meters, the height of the container:");
    scanf("%f", &contheight);

    printf("\nNow, insert its radius:");
    scanf("%f", &radius);

    volume = 3.14 * (radius * radius) * contheight;
    printf("\nWith the given info, the volume of the container is: %.2f.", volume);

    return 0;

}
